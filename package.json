{
  "name": "chrome-extension-site-blocker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "check": "biome check --apply .",
    "tsc": "tsc --skipLibCheck --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx}": ["bash -c \"npm run tsc\"", "bash -c \"npm run check\""]
  },
  "dependencies": {
    "@mantine/core": "^7.7.1",
    "@mantine/form": "^7.7.1",
    "@mantine/hooks": "^7.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@crxjs/vite-plugin": "^2.0.0-beta.23",
    "@types/chrome": "^0.0.263",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
